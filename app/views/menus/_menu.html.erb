
	<li div="menus" data-id="<%= menu.id %>" data-time="<%= menu.created_at.to_i %>">
			<% if menu.starter %>
			<div> <%= "Starter : " + menu.starter  %> </div>
			<div> <%= "ID : " + menu.id.to_s  %> </div>
			<div> <%= "created at : " + menu.created_at.to_i.to_s %> </div>
			<% end %>
			<% if menu.main_course %>
			<div> <%= "Main Course: " + menu.main_course %> </div>
			<% end %>
			<% if menu.desserts %>
			<div> <%= "Desserts: " + menu.desserts %> </div>
			<% end%>
			<% if menu.cuisines %>
			<div> <%= "Type of Cuisine: " + menu.cuisines %> </div>
			<% end%>
	</li>	
